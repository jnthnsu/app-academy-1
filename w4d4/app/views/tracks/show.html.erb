<%= render 'shared/errors' %>
<%= button_to "back to album.", album_url(@track.album), method: :get %>
<h1> <%= @track.name %></h1>
<%= ugly_lyrics(@track.lyrics) %>

<% @track.notes.each do |note| %>
  <%= render 'notes/note', note: note %>
<% end %>
<%= render 'notes/form', track: @track %>

<%= link_to "edit track.", edit_track_url(@track), method: :get %>
<%= link_to "delete band.", track_url(@track), method: :delete %>
